{
  "name": "circle-advanced-setup-workflow",
  "packageManager": "yarn@3.6.3",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "clean": "find . -name node_modules -o -name .turbo -o -name dist -type d -prune | xargs rm -rf"
  },
  "devDependencies": {
    "turbo": "^1.10.7"
  }
}
